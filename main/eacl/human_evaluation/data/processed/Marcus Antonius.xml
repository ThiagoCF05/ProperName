			<REFEX FORM="" MODEL="original">
			Antony
		</REFEX>
		</REFERENCE>
		<STRING> married Octavian's sister, Octavia. Despite this marriage, </STRING>
		<REFERENCE GIVENNESS="old" ID="8" SENTENCE-GIVENNESS="new" SYNCAT="np-subj">
			<REFEX FORM="+f" MODEL="random">Mark</REFEX>
			<REFEX FORM="+l" MODEL="siddharthan">Antony</REFEX>
			<REFEX FORM="+l" MODEL="bayes_no_variation">Antony</REFEX>
